{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/luxon": "^2.0.9",
    "@types/node": "^16.11.24",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "axios": "^1.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "luxon": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.5",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:dev": "concurrently --kill-others 'npm run start:dev:frontend' 'npm run start:dev:backend'",
    "start:dev:frontend": "react-scripts start",
    "start:dev:backend": "nodemon ./src/backend/server.ts",
    "start:prod:backend": "node ./dist/server.js",
    "build:frontend": "export PUBLIC_URL=./ && react-scripts build",
    "build:backend": "tsc -p ./src/backend/tsconfig.json",
    "renderer:build": "npm install && npm run build:backend",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/uuid": "^9.0.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  }
}
